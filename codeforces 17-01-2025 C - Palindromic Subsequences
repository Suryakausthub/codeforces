#include <bits/stdc++.h>
using namespace std;

typedef long long ll;
typedef double dou;
typedef const int ci;
typedef pair<int, int> pii;

ci maxn = 2e6 + 10;

int n, t;

int main() {
    scanf("%d", &t);

    while (t--) {
        scanf("%d", &n);
        printf("1 ");
        for (int i = 2; i <= n - 2; i++) {
            printf("%d ", i - 1);
        }
        printf("1 2\n");
    }

    return 0;
}
